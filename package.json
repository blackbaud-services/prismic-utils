{
  "name": "prismic-utils",
  "version": "5.0.0",
  "description": "A collection of functions for parsing data from a Prismic CMS",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel source -d dist --ignore '**/__tests__/*'",
    "docs": "documentation build source/** -f html -o docs",
    "docs:publish": "npm run docs && gh-pages -d docs",
    "docs:watch": "nodemon --ignore docs/ --exec 'npm run docs'",
    "prepublish": "npm test && npm run build",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "standard -d source",
    "test:unit": "mocha source/**/**/__tests__/*.js --compilers js:babel-register"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everydayhero/prismic-utils.git"
  },
  "author": "@everydayhero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/everydayhero/prismic-utils/issues"
  },
  "homepage": "https://github.com/everydayhero/prismic-utils#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.9.0",
    "documentation": "^5.3.3",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.2.0",
    "jsdom": "^11.3.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prismic-dom": "^2.0.6",
    "prismic-javascript": "^1.4.1",
    "prismic-reactjs": "^0.2.3",
    "react-dom": "^15",
    "sinon": "^2.3.2",
    "striptags": "^2.1.1"
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/flow-typed/"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
